<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<component>
	<extend>uicomponent.expand.component.chooseOrg</extend>
    <modelId>req.uicomponent.expand.component.chooseOrg</modelId>
    <modelName>ChooseOrg</modelName>
    <modelPackage>req.uicomponent.expand</modelPackage>
    <modelType>component</modelType>
	<useRange>req</useRange>
	<cname>组织选择标签</cname>
	<ename>ChooseOrg</ename>
	<group>expand</group>
	<description>ChooseOrg组件</description>
	<propertyEditor>auto</propertyEditor>
	<propertyEditorPage></propertyEditorPage>
	<hasInputType>true</hasInputType>
	<js>
		<list>common/choose/js/choose.js</list>
		<list>common/choose/js/messenger.js</list>
	</js>
	<css>
		<list>common/choose/css/choose.css</list>
	</css>
	<properties>
		<property>
			<cname>值</cname>
			<ename>value</ename>
			<type>String</type>
			<requried>false</requried>
			<default></default>
