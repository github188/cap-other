			<commonAttr>true</commonAttr>
			<!--文本框-->
			<propertyEditorUI>
				<componentName>cui_input</componentName>
				<script>
				<![CDATA[ 
					{
						'id': 'value',
						'name': 'value',
						'ng-model':'data.value'
					}
				]]>
				</script>
			</propertyEditorUI>
		</property>
		<property>
			<ename>idName</ename>
			<hide>true</hide>
		</property>
		<property>
			<ename>valueName</ename>
			<hide>true</hide>
		</property>
		<property>
			<ename>defaultOrgId</ename>
			<hide>true</hide>
		</property>
		<property>
			<ename>rootId</ename>
			<hide>true</hide>
		</property>
		<property>
			<ename>orgStructureId</ename>
			<hide>true</hide>
		</property>
		<property>
			<ename>isSearch</ename>
			<hide>true</hide>
		</property>
		<property>
			<ename>opts</ename>
			<hide>true</hide>
		</property>
	</properties>
</component>
