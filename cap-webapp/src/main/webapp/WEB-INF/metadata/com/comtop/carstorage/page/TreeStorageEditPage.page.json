			"methodBody":"\t\t\n//页面初始化数据加载行为 页面初始化数据加载行为\nfunction pageInitLoadData(){\n\t//TODO 加载数据前操作\n     \n\tvar paramArray =[];\n\tcap.formId = primaryValue;\n\tvar param = \"primaryValue\";\n    if (param) {\n       paramArray = param.split(\",\");\n    }\n\tif(!cap.isUndefinedOrNullOrBlank(cap.formId) && paramArray.length>0){\n\t\t\n\t\tvar dwrInvokeParam = cap.getDwrInvokeParam('com.comtop.carstorage.entity.Carstorage','loadById',paramArray);\n\t\tdwr.TOPEngine.setAsync(false);\n\t\tTreeStorageEditPageAction.dwrInvoke(dwrInvokeParam,{callback:function(result){\n\t\t\t//TODO 数据设置前操作\n\n\t\t\tcarstorage=result;\n\t\t\t//TODO 数据设置后操作\n            carstorage[\"treeId\"]= result[\"id\"];\n            carstorage[\"treeName\"]= result[\"name\"];\n\t\t},\n\t\terrorHandler:function(message, exception){\n\t\t   //TODO 后台异常信息回调\n\n\t\t}\n\t\t});\n\t\tdwr.TOPEngine.setAsync(true);\n\t}else{\n\t\tvar defaultValue = {};\n\t\tvar expression = \"default\";\n\t\t//TODO 改变expression\n\n\t\tswitch (expression){\n\t\t\tcase \"default\":\n\t\t\t\tdefaultValue = <%=com.comtop.cip.json.JSON.toJSONStringWithDateFormat(request.getAttribute(\"carstorage\"),\"yyyy-MM-dd HH:mm:ss\")%>;\n\t\t\t\tbreak;\n\t\t\t//TODO 增加case控制\n\n\t\t}\n\t\tcarstorage = defaultValue ? defaultValue : {};\n\t}\n\t//TODO 数据加载完成后操作\n    if(parentId){\n              carstorage[\"treeParentId\"]=parentId;\n            }\n}\n\t\n",
