			"methodBody":"\t\t\n//编辑链接渲染 grid编辑链接渲染\nfunction gridEditLinkRender(rd, index, col) { \n\tif(!rd[col.bindName]){\n\t\treturn;\n\t}\n\tvar url=cap.getforwardURL(otcWorkflowEditPage);\n\turl=cap.buildURL(url,{primaryValue:rd['primaryValue']});\n\t//自定义URL内容\n    url=cap.buildURL(url,{taskId:rd['taskId']});\n\turl=cap.buildURL(url,{taskType: 'done'});\n\tvar content= \"<a style=\\\"cursor:pointer\\\" onclick=\\\"cap.pageJump('\"+url+\"','win',window)\\\">\" + rd[col.bindName] + \"</a>\";\n\t//自定义content内容\n\n\treturn content;\n}\n\t\n",
