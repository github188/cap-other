			"methodBody":"\n\t\t\n//删除行为 删除行为\nfunction deleteRow(){\n\t//删除前操作\n\t\n\tvar gridId=\"uiid-2007403086638078\";\n\tvar gridObj = cui(\"#\"+gridId);\n\tvar selects = gridObj.getSelectedRowData();\n\tif(selects == null || selects.length == 0){\n\t\tvar deletemsg = \"请选择要删除的数据。\";\n\t\t\n\t\tcui.alert(deletemsg);\n\t\treturn;\n\t}\n\tvar confirmmsg = \"确定要删除这\"+selects.length+\"条数据吗？\";\n\t\n\tcui.confirm(confirmmsg,{\n\t\tonYes:function(){\n\t\t\tvar paramArray = [];\n\t\t\tparamArray[0] = selects;\n\t\t\tvar aliasEntityId = 'com.comtop.user.entity.User3';\n\t\t\taliasEntityId = aliasEntityId != '' ? aliasEntityId :  'com.comtop.user.entity.User';\t\n\t\t\tvar dwrInvokeParam = cap.getDwrInvokeParam(aliasEntityId,'deleteList',paramArray);\n\t\t\tUserListPageAction.dwrInvoke(dwrInvokeParam,{callback:function(result){\n\t\t\t\t//数据刷新前处理\n\t\t\t\t\n\t\t\t\t\n\t\t\t\tif(result){\n\t\t\t\t\tgridObj.loadData();\n\t\t\t\t\tvar resultmsg = \"删除成功！\";\n\t\t\t\t\t\n\t\t\t\t \tcui.message(resultmsg,\"success\");\n\t\t\t\t}else{\n\t\t\t\t\tvar resultFalsemsg = \"删除失败，请重新操作！\";\n\t\t\t\t\t\n\t\t\t\t\tcui.error(resultFalsemsg);\n\t\t\t\t}\n\t\t\t\t\n\t\t\t\t//数据刷新后处理\n\t\t\t\t\n\t\t\t},\n\t\t\terrorHandler:function(message, exception){\n\t\t\t   //TODO 后台异常信息回调\n\t\t\t   var message =\"删除失败,存在异常信息:\"+message;\n\t\t\t   \n\t\t\t   cui.message(message);\n\t\t\t}\n\t\t\t});\n\t\t}\n\t});\n\t//TODO 可自定义设置返回值\n\t\n}\n\t",
