			"ename":"moreCondition",
			"methodBody":"\n\t\t\n/*\n * 更多查询 更多查询\n *\n * @param event 当前事件对象\n * @param self 当前button对象\n * @param mark 传入的参数\n */\nfunction moreCondition(event, self, mark) {\n\tif(!self){\n\t\tcui(\"#tableid-4294206428807329\").hide();\n\t\treturn; \n\t}\n\tvar btnName=self.getLabel();\n\tif(btnName=='更多▼'){\n\t\tself.setLabel('更多▲');\n\t\tcui(\"#tableid-4294206428807329\").show();\n\t}else if(btnName=='更多▲'){\n\t\tself.setLabel('更多▼');\n\t\tcui(\"#tableid-4294206428807329\").hide();\n\t}\n}\n\t\t\n\t",
