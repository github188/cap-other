			"methodBody":"\n\t\t\n//页面初始化数据加载行为 页面初始化数据加载行为\nfunction pageInitLoadData(){\n\t//TODO 加载数据前操作\n\t\n\tvar paramArray =[];\n\tcap.formId = primaryValue;\n\tvar param = \"primaryValue\";\n    if (param) {\n       paramArray = param.split(\",\");\n    }\n\tif(!cap.isUndefinedOrNullOrBlank(cap.formId) && paramArray.length>0){\n\t\tvar aliasEntityId = 'com.comtop.budget.entity.BmsBudget';\n\t\taliasEntityId = aliasEntityId != '' ? aliasEntityId :  'com.comtop.budget.entity.BmsBudget';\n\t\tvar dwrInvokeParam = cap.getDwrInvokeParam(aliasEntityId,'loadById',paramArray);\n\t\tdwr.TOPEngine.setAsync(false);\n\t\tBmsBudgetEditPageAction.dwrInvoke(dwrInvokeParam,{callback:function(result){\n\t\t\t//TODO 数据设置前操作\n\t\t\t\n\t\t\tbmsBudget=result;\n\t\t\t//TODO 数据设置后操作\n\t\t\t\n\t\t},\n\t\terrorHandler:function(message, exception){\n\t\t   //TODO 后台异常信息回调\n\t\t   \n\t\t}\n\t\t});\n\t\tdwr.TOPEngine.setAsync(true);\n\t}else{\n\t\tvar defaultValue = {};\n\t\tvar expression = \"default\";\n\t\t//TODO 改变expression\n\t\t\t\n\t\tswitch (expression){\n\t\t\tcase \"default\":\n\t\t\t\tdefaultValue = <%=com.comtop.cip.json.JSON.toJSONStringWithDateFormat(request.getAttribute(\"bmsBudget\"),\"yyyy-MM-dd HH:mm:ss\")%>;\n\t\t\t\tbreak;\n\t\t\t//TODO 增加case控制\n\t\t\t\t\n\t\t}\n\t\tbmsBudget = defaultValue ? defaultValue : {};\n\t}\n\t//TODO 数据加载完成后操作\n\t\n}\n\t",
