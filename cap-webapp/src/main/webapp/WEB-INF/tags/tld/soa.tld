<?xml version="1.0" encoding="UTF-8"?>
<taglib xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd"
	version="2.0">

	<description>TopUI标签库</description>
	<tlib-version>1.0</tlib-version>
	<short-name>cui</short-name>
	<uri>http://www.szcomtop.com/soa/cui</uri>
	<!-- web应用根路径 -->
	<tag>
		<description>
    		<![CDATA[<b>组件名称：</b>webRoot<br><br>
	    	用于输出web应用根路径地址
			]]>
    	</description>
		<name>webRoot</name>
		<tag-class>com.comtop.soa.cui.WebRootTag</tag-class>
		<body-content>empty</body-content>
	</tag>
	<!-- 服务器ip -->
	<tag>
		<description>
    		<![CDATA[<b>组件名称：</b>serverIp<br><br>
	    	输出服务器IP地址
			]]>
		</description>
		<name>serverIp</name>
		<tag-class>com.comtop.soa.cui.ServerIpTag</tag-class>
		<body-content>empty</body-content>
	</tag>
	<!-- 清楚缓存 -->
	<tag>
		<description>
    		<![CDATA[<b>组件名称：</b>noCache<br><br>
	    	清除缓存标签<br>
			]]>
    	</description>
		<name>noCache</name>
		<tag-class>com.comtop.soa.cui.NoCacheTag</tag-class>
		<body-content>empty</body-content>
	</tag>
	<!-- 获取请求信息 -->
	<tag>
		<description>
	    	<![CDATA[<b>组件名称：</b>info<br><br>
	    		获取请求的相关信息，应用于页面最前头隐藏的信息，一般只用在tabligs.jsp。
	    	]]>
    	</description>
		<name>info</name>
		<tag-class>com.comtop.soa.cui.InfoTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<description>
      			<![CDATA[<b>属性名称：</b>type<br><br>
	      		请求信息的类型，fileName||address||url
	      		]]>
      		</description>
			<name>type</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
	</tag>
	
	<!-- 引入css -->
	<tag>
		<description>
    		<![CDATA[<b>组件名称：</b>link<br><br>
	    	用于引入css
			]]>
    	</description>
		<name>link</name>
		<tag-class>com.comtop.soa.cui.LinkTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<description>
      			<![CDATA[<b>属性名称：</b>href<br><br>
	      		引入css的地址
	      		]]>
      		</description>
			<name>href</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
	</tag>
	
	<!-- 引入js -->
	<tag>
		<description>
    		<![CDATA[<b>组件名称：</b>script<br><br>
	    	用于引入js
			]]>
    	</description>
		<name>script</name>
		<tag-class>com.comtop.soa.cui.ScriptTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<description>
      			<![CDATA[<b>属性名称：</b>src<br><br>
	      		引入js的地址
	      		]]>
      		</description>
			<name>src</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>
      			<![CDATA[<b>属性名称：</b>cuiTemplate<br><br>
	      		引入temple的名字,comtop.ui.base扩展自定义模板时候用的
	      		]]>
      		</description>
			<name>cuiTemplate</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
	</tag>

	<!-- button 按钮组件 -->
	<tag>
		<description>
	    	<![CDATA[<b>组件名称：</b>button<br><br>
	    	按钮的唯一标识，需要引用菜单的时候需要用户自己填写
			]]>
    	</description>
		<name>button</name>
		<tag-class>com.comtop.soa.cui.ButtonTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>
	      		<![CDATA[<b>属性名称：</b>id<br><br>
	      		组件id
	      		]]>
			</description>
			<name>id</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>
	      		<![CDATA[<b>属性名称：</b>label<br><br>
	      		按钮的标题，显示给用户看的文字。
	      		]]>
			</description>
			<name>label</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>
	      		<![CDATA[<b>属性名称：</b>hide<br><br>
	      		是否隐藏按钮(隐藏后看不见按钮)。 
	      		]]>
			</description>
			<name>hide</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.Boolean</type>
		</attribute>
		<attribute>
			<description>
	      		<![CDATA[<b>属性名称：</b>disable<br><br>
				是否禁用按钮(禁用后按钮变成灰色，点击不可用)。      		
	      		]]>
			</description>
			<name>disable</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.Boolean</type>
		</attribute>
		<attribute>
			<description>
	      		<![CDATA[<b>属性名称：</b>icon<br><br>
	      		设置按钮图标。<br><br>
				设置图标方法：<br><br>
				1、如果图标是存放在cui文件夹的"themes/default/images/button"路径下面，则icon="图标文件名(如mail.gif)"<br><br>
				2、如果图标不在上述指定位置，则icon的值必须是一个绝对路径或是有效可访问的地址。<br><br>
				注意：<br><br>
				1、图标支持png,jpg,jpeg,gif格式。<br><br>
				2、最理想的图标尺寸是16px*16px。 
	      		]]>
			</description>
			<name>icon</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>
	      		<![CDATA[<b>属性名称：</b>mark<br><br>
				给当前Button一个唯一的标识，一般用来给on_click的回调传参数。     		
	      		]]>
			</description>
			<name>mark</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>
	      		<![CDATA[<b>属性名称：</b>on_click<br><br>
	      		单击事件
	      		]]>
			</description>
			<name>on_click</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>
	      		<![CDATA[<b>属性名称：</b>menu<br><br>
	      		菜单的options数据
	      		]]>
			</description>
			<name>menu</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
	</tag>


	<!-- menu 组件 -->
	<tag>
		<description>
	       	<![CDATA[<b>组件名称：</b>menu<br><br>
	    	用来做下来菜单操作的组件。
			]]>
    	</description>
		<name>menu</name>
		<tag-class>com.comtop.soa.cui.MenuTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>
				<![CDATA[<b>属性名称：</b>id<br><br>
	      		菜单的唯一标识，需要引用菜单的时候需要用户自己填写
	      		]]>
			</description>
			<name>id</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>
				<![CDATA[<b>属性名称：</b>trigger<br><br>
	      		触发菜单弹出的方式（click或者mouseover）。
	      		]]>
    		</description>
			<name>trigger</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>
				<![CDATA[<b>属性名称：</b>width<br><br>
	      		菜单的宽度，如果不设置，与占位符宽度一致。
	      		]]>
    		</description>
			<name>width</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>
				<![CDATA[<b>属性名称：</b>datasource<br><br>
	      		数据源，json数组或者返回json数组的方法
	      		]]>
    		</description>
			<name>datasource</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>
				<![CDATA[<b>属性名称：</b>on_click<br><br>
	      		点击响应事件
	      		]]>
    		</description>
			<name>on_click</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
	</tag>
	
	
	<!-- MultiNav 组件 -->
	<tag>
		<description>
	       	<![CDATA[<b>组件名称：</b>multiNav<br><br>
	    	MultiNav组件是一个多功能导航组件，支持普通的一级菜单，也支持二级甚至无限级菜单.类似tree，支持多种事件绑定方式
			]]>
    	</description>
		<name>multiNav</name>
		<tag-class>com.comtop.soa.cui.MultiNavTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>
				<![CDATA[<b>属性名称：</b>id<br><br>
	      		菜单的唯一标识，需要引用菜单的时候需要用户自己填写
	      		]]>
			</description>
			<name>id</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>
				<![CDATA[<b>属性名称：</b>datasource<br><br>
	      		菜单json数据
	      		]]>
    		</description>
			<name>datasource</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>
				<![CDATA[<b>属性名称：</b>unfold<br><br>
	      		多级导航有效,是否全部展开子菜单
	      		]]>
			</description>
			<name>unfold</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.Boolean</type>
		</attribute>
		<attribute>
			<description>
				<![CDATA[<b>属性名称：</b>initchecked<br><br>
	      		默认选中的菜单,比如[0, 1]表示第1个子菜单下的第2个子菜单被初始选中<br><br>
				如果被选中的菜单是目录菜单(有子菜单的菜单)，<br><br>
				那么会根据foldercheckable属性判断是否有选中状态。
	      		]]>
    		</description>
			<name>initchecked</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>
				<![CDATA[<b>属性名称：</b>target<br><br>
	      		菜单链接情况下的默认target属性,这里支持iframe的name属性.菜单数据json没设置target用这个默认值.
	      		]]>
    		</description>
			<name>target</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>
				<![CDATA[<b>属性名称：</b>foldercheckable<br><br>
	      		多级导航有效,目录菜单（有子菜单的菜单）是否有被选中状态。
	      		]]>
    		</description>
			<name>foldercheckable</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.Boolean</type>
		</attribute>
	</tag>

	<!-- input 组件 -->
	<tag>
		<description>
	    	<![CDATA[<b>组件名称：</b>input<br><br>
	    	input组件
			]]>
    	</description>
		<name>input</name>
		<tag-class>com.comtop.soa.cui.InputTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<description>
	      		<![CDATA[<b>属性名称：</b>id<br><br>
	      		文本框的唯一标识，需要引用菜单的时候需要用户自己填写
	      		]]>
			</description>
			<name>id</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>
	      		<![CDATA[<b>属性名称：</b>name<br><br>
	      		input的name属性
	      		]]>
			</description>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>
	      		<![CDATA[<b>属性名称：</b>width<br><br>
				input的宽度
	      		]]>
			</description>
			<name>width</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>
	      		<![CDATA[<b>属性名称：</b>maxlength<br><br>
	      		input框允许输入的最大长度（单位：字符）
	      		]]>
			</description>
			<name>maxlength</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.Integer</type>
		</attribute>
		<attribute>
			<description>
	      		<![CDATA[<b>属性名称：</b>value<br><br>
	      		input框默认值
	      		]]>
			</description>
			<name>value</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>
	      		<![CDATA[<b>属性名称：</b>align<br><br>
	      		input框文字对齐方式，可选值有：'left'、'center'、'right'。
	      		]]>
			</description>
			<name>align</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>
	      		<![CDATA[<b>属性名称：</b>readonly<br><br>
	      		是否只读
	      		]]>
			</description>
			<name>readonly</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.Boolean</type>
		</attribute>
		<attribute>
			<description>
	      		<![CDATA[<b>属性名称：</b>emptytext<br><br>
	      		input框没有值时显示的提示内容。
	      		]]>
			</description>
			<name>emptytext</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>
	      		<![CDATA[<b>属性名称：</b>type<br><br>
	      		input框类型，可选值有：'text'、'password'
	      		]]>
			</description>
			<name>type</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>
	      		<![CDATA[<b>属性名称：</b>mask<br><br>
	      		输入框输入格式模板，如：<br><br>

			    Datetimes：日期时间模板包含秒，如'2012-02-03 14:20:56'<br><br>
			    Times：时间模板，如'14:20:56'<br><br>
			    Time：时间模板，不包含秒，如'14:20'<br><br>
			    Datetime：日期时间模板，不包含秒，如'2012-02-03 14:20'<br><br>
			    Date：日期模板，如'2012-02-03'<br><br>
			    Int：整数，如'11845179'<br><br>
			    Dec：小数，如'12584.0214'<br><br>
			    Num：数字，如'024512'<br><br>
			    Money：货币格式，默认保留两位小数，如'111,111,111.11'<br><br>
		    	当默认的模板不满足要求时，可以自定义模板（需设置mask="custom"，再结合maskoptions使用）。
	      		]]>
			</description>
			<name>mask</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>
	      		<![CDATA[<b>属性名称：</b>maskoptions<br><br>
	      		自定义模板属性。 
	      		]]>
			</description>
			<name>maskoptions</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>
	      		<![CDATA[<b>属性名称：</b>textmode<br><br>
	      		是否为文本模式 
	      		]]>
			</description>
			<name>textmode</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.Boolean</type>
		</attribute>
		<attribute>
			<description>
	      		<![CDATA[<b>属性名称：</b>tip<br><br>
	      		添加提示
	      		]]>
			</description>
			<name>tip</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>
	      		<![CDATA[<b>属性名称：</b>validate<br><br>
	      		添加验证
	      		]]>
			</description>
			<name>validate</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>
	      		<![CDATA[<b>属性名称：</b>databind<br><br>
	      		数据绑定
	      		]]>
			</description>
			<name>databind</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>
	      		<![CDATA[<b>属性名称：</b>on_change<br><br>
	      		change事件
	      		]]>
			</description>
			<name>on_change</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>
	      		<![CDATA[<b>属性名称：</b>on_focus<br><br>
	      		focus事件
	      		]]>
			</description>
			<name>on_focus</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>
	      		<![CDATA[<b>属性名称：</b>on_blur<br><br>
	      		blur事件
	      		]]>
			</description>
			<name>on_blur</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>
	      		<![CDATA[<b>属性名称：</b>on_keyup<br><br>
	      		keyup事件
	      		]]>
